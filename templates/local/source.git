e2source {
	type = "git",
	-- environment
	env = {
		--key = "value",
	},
	-- a list of licences from proj/licences
	licences = { },
	-- git specific attributes
	-- server name
	server = "proj-storage",
	-- the repository location relative to the server
	location = "git/<template>.git",
	-- git branch name
	branch = "master",
	-- git tag names, the first one is used for building
	tag = "<template>",
}
